{
  "name": "biuro-app",
  "version": "0.1.20",
  "main": "dist/main.js",
  "description": "biurowy mikro kombajn",
  "author": "≈Åukasz Bulak",
  "repository": {
    "type": "git",
    "url": "https://github.com/theMooja/BiuroApp.git"
  },
  "scripts": {
    "build-electron": "tsc --project projects/electron/tsconfig.json && npm run copy-files",
    "ng": "ng",
    "start": "concurrently \"npm run build-electron\" \"npm run serve-app\" \"npm run wait-for-app && electron-forge start\"",
    "build-app": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "wait-for-app": "wait-on http://localhost:4200",
    "serve-app": "ng serve BiuroApp",
    "copy-files": "copyfiles app-settings.json dist/",
    "package": "npm run build-electron && npm run build-app && electron-forge package",
    "make": "npm run build-electron && npm run build-app && electron-forge make",
    "release": "npm run build-electron && npm run build-app && electron-forge publish"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.1",
    "@angular/material-date-fns-adapter": "^17.3.7",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@electron-forge/publisher-github": "^7.6.0",
    "angular2-notifications": "^16.0.1",
    "date-fns": "^3.6.0",
    "electron-log": "^5.2.4",
    "electron-settings": "^4.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.3.9",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.10.38",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.20",
    "update-electron-app": "^3.1.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular/cli": "^17.3.2",
    "@angular/compiler-cli": "^17.3.0",
    "@electron-forge/cli": "^7.3.1",
    "@electron-forge/maker-deb": "^7.3.1",
    "@electron-forge/maker-rpm": "^7.3.1",
    "@electron-forge/maker-squirrel": "^7.3.1",
    "@electron-forge/maker-zip": "^7.3.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.11.10",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "electron": "^29.1.5",
    "electron-builder": "^25.1.8",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2",
    "wait-on": "^7.2.0"
  }
