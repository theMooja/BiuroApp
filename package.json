{
  "name": "biuro-app",
  "version": "0.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build-electron": "tsc --project projects/electron/tsconfig.json",
    "ng": "ng",
    "start": "concurrently \"npm run build-electron\" \"npm run serve-app\" \"npm run wait-for-app && electron-forge start\"",
    "build-app": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "wait-for-app": "wait-on http://localhost:4200",
    "serve-app": "ng serve BiuroApp",
    "package": "npm run build-electron && npm run build-app && electron-forge package",
    "make": "npm build-electron && npm build-app && electron-forge make",
    "publish-app": "npm build-electron && npm build-app && electron-forge publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.1",
    "@angular/material-date-fns-adapter": "^17.3.7",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "electron-settings": "^4.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "mongoose": "^8.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular/cli": "^17.3.2",
    "@angular/compiler-cli": "^17.3.0",
    "@electron-forge/cli": "^7.3.1",
    "@electron-forge/maker-deb": "^7.3.1",
    "@electron-forge/maker-rpm": "^7.3.1",
    "@electron-forge/maker-squirrel": "^7.3.1",
    "@electron-forge/maker-zip": "^7.3.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.2",
    "electron": "^29.1.5",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2",
    "wait-on": "^7.2.0"
  }
}
